{
  "name": "@blackbear-tech/v-calendar",
  "type": "module",
  "version": "1.0.4",
  "description": "A calendar and date picker plugin for Vue.js.",
  "author": "Nathan Reyes <nathanreyes@me.com>",
  "repository": "https://github.com/blackbear-global/v-calendar",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "jsdelivr": "dist/iife/index.js",
  "unpkg": "dist/iife/index.js",
  "types": "dist/types/src/index.d.ts",
  "keywords": [
    "vue",
    "vue3",
    "vuejs",
    "plugin",
    "calendar",
    "v-calendar",
    "datepicker",
    "date picker"
  ],
  "license": "MIT",
  "files": [
    "src",
    "dist",
    "package.json",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/src/index.d.ts",
      "import": {
        "node": "./dist/mjs/index.mjs",
        "default": "./dist/es/index.js"
      },
      "require": "./dist/cjs/index.js"
    },
    "./style.css": "./dist/style.css",
    "./dist/*": "./dist/*"
  },
  "scripts": {
    "build": "tsx build/build.ts",
    "build:docs": "cd docs && yarn build",
    "build:all": "yarn build && yarn build:docs",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,vue}\"",
    "lint": "eslint . --ext .js,.vue",
    "test": "TZ=UTC vitest"
  },
  "dependencies": {
    "vue-screen-utils": "^1.0.0-beta.13"
  },
  "peerDependencies": {
    "@floating-ui/dom": "^1.5.3",
    "date-fns": "^3.0.0",
    "date-fns-tz": "^3.0.0",
    "lodash": "^4.17.20",
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@floating-ui/dom": "^1.5.3",
    "@optimize-lodash/rollup-plugin": "^4.0.4",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/lodash": "^4.14.165",
    "@types/node": "^20.12.7",
    "@types/resize-observer-browser": "^0.1.7",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/compiler-sfc": "^3.2.0",
    "@vue/eslint-config-prettier": "6.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.3.2",
    "autoprefixer": "^9.7.8",
    "date-fns": "^3.0.0",
    "date-fns-tz": "^3.0.0",
    "eslint": "^8.17.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.1.1",
    "jsdom": "^24.0.0",
    "lint-staged": "^10.4.2",
    "lodash": "^4.17.20",
    "postcss": "^8.0.0",
    "postcss-import": "^14.0.2",
    "postcss-inline-svg": "^4.1.0",
    "postcss-nested": "^4.2.1",
    "postcss-simple-vars": "5.0.2",
    "postcss-url": "^8.0.0",
    "prettier": "^3.0.3",
    "regenerator-runtime": "^0.13.9",
    "resize-observer-polyfill": "^1.5.1",
    "rollup-plugin-visualizer": "^5.5.4",
    "terser": "^5.16.1",
    "tslib": "^2.4.0",
    "tsx": "^3.12.2",
    "typescript": "^4.8.4",
    "vite": "^4.0.4",
    "vitest": "^1.4.0",
    "vue": "^3.2.0",
    "vue-tsc": "^2.0.11"
  },
  "sideEffects": false,
  "packageManager": "yarn@1.22.21"
}
